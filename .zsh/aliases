alias v="nvim"
alias zshcfg="v ~/.zshrc"
alias alicfg="v ~/.zsh/aliases"
alias envcfg="v ~/.zsh/env"
alias vimcfg="v ~/.vimrc"
alias vi="v"
alias vim="v"
alias v.="v ."
alias gs="git status"
alias rpr="cd ~/projects/ruby"
alias ppr="cd ~/projects/python"
alias ber="bundle exec rspec"
alias befr="bundle exec rspec --only-failures"
alias br="bin/rspec"
alias bfr="bin/rspec --only-failures"
alias nice_log="git log --oneline --decorate --graph --all"
